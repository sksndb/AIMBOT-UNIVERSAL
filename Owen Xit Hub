})
AimbotTab:CreateSlider({
Name = "Smoothness (0 = instant)",
Range = {0,1.5},
Increment = 0.05,
CurrentValue = 0,
Flag = "Aimbot_Smoothness",
Callback = function(v) Env.Settings.Sensitivity = v end
})
VisualsTab:CreateToggle({
Name = "Enable ESP",
CurrentValue = false,
Flag = "ESP_Enabled",
Callback = function(v) ESPSettings.Enabled = v end
})
VisualsTab:CreateToggle({
Name = "Boxes",
CurrentValue = true,
Flag = "ESP_Boxes",
Callback = function(v) ESPSettings.Boxes = v end
})
VisualsTab:CreateToggle({
Name = "Tracers",
CurrentValue = true,
Flag = "ESP_Tracers",
Callback = function(v) ESPSettings.Tracers = v end
})
VisualsTab:CreateToggle({
Name = "Names + HP + Dist",
CurrentValue = true,
Flag = "ESP_Names",
Callback = function(v) ESPSettings.Names = v end
})
VisualsTab:CreateToggle({
Name = "Skeleton",
CurrentValue = true,
Flag = "ESP_Skeleton",
Callback = function(v) ESPSettings.Skeleton = v end
})
VisualsTab:CreateToggle({
