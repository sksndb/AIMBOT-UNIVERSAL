-- Download the remote key (with fallback if internet or pastebin fails)
local CorrectKey = "fallback_key" -- just in case it doesn't load
pcall(function()
Â Â Â Â local response = game:HttpGet(KeyUrl, true) -- true = no cache
Â Â Â Â if response and response ~= "" then
Â Â Â Â Â Â Â Â CorrectKey = response:match("^%s*(.-)%s*$") or CorrectKey
Â Â Â Â end
end)
-- Local file to persist (doesn't ask for key every server hop)
local KeyFile = "owen_xit_key.txt"
local function LoadSavedKey()
Â Â Â Â if isfile and isfile(KeyFile) then
Â Â Â Â Â Â Â Â local content = readfile(KeyFile)
Â Â Â Â Â Â Â Â return content:match("^%s*(.-)%s*$") or ""
Â Â Â Â end
Â Â Â Â return nil
end
local function SaveKey(key)
Â Â Â Â if writefile then
Â Â Â Â Â Â Â Â writefile(KeyFile, key)
Â Â Â Â end
end
