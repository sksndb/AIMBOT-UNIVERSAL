RunService.Heartbeat:Connect(function()
Â Â Â Â if not Running or not Env.Settings.Enabled or not Env.Locked then return end
Â Â Â Â local char = Env.Locked.Character local part = char and char:FindFirstChild(Env.Settings.LockPart)
Â Â Â Â if not part then if CurrentTween then CurrentTween:Cancel() CurrentTween = nil end Env.Locked = nil Running = false return end
Â Â Â Â local target = part.Position local scr = Camera:WorldToViewportPoint(target)
Â Â Â Â if Env.Settings.LockMode == 2 then
Â Â Â Â Â Â Â Â mousemoverel((scr.X - UIS:GetMouseLocation().X)/Env.Settings.Sensitivity2, (scr.Y - UIS:GetMouseLocation().Y)/Env.Settings.Sensitivity2)
