local function CreateESP(p)
Â Â Â Â if ESPObjects[p] then return end
Â Â Â Â local char = p.Character or p.CharacterAdded:Wait()
Â Â Â Â if not char then return end
Â Â Â Â local data = {Links = char:FindFirstChild("UpperTorso") and R15Links or R6Links}
Â Â Â Â if ESPSettings.Boxes then data.Box = Drawing.new("Square") data.Box.Filled = false data.Box.Transparency = 1 data.Box.Thickness = ESPSettings.Thickness end
Â Â Â Â if ESPSettings.Tracers then data.Tracer = Drawing.new("Line") data.Tracer.Thickness = 1.5 data.Tracer.Transparency = 1 end
Â Â Â Â if ESPSettings.Names then data.Name = Drawing.new("Text") data.Name.Center = true data.Name.Outline = true data.Name.OutlineColor = Color3.new(0,0,0) data.Name.Font = 2 data.Name.Size = ESPSettings.TextSize data.Name.Transparency = 1 end
Â Â Â Â if ESPSettings.HealthBars then
Â Â Â Â Â Â Â Â data.HBBG = Drawing.new("Square") data.HBBG.Filled = true data.HBBG.Color = Color3.new(0,0,0) data.HBBG.Transparency = 0.6
Â Â Â Â Â Â Â Â data.HB = Drawing.new("Square") data.HB.Filled = true data.HB.Transparency = 1
Â Â Â Â end
Â Â Â Â data.SkelLines = {}
Â Â Â Â if ESPSettings.Skeleton then for _ = 1, #data.Links do local ln = Drawing.new("Line") ln.Thickness = 1.8 ln.Transparency = 0.7 table.insert(data.SkelLines, ln) end end
Â Â Â Â data.Highlight = Instance.new("Highlight") data.Highlight.FillTransparency = 0.4 data.Highlight.OutlineTransparency = 0 data.Highlight.Parent = Camera
Â Â Â Â ESPObjects[p] = data
end
local function RemoveESP(p)
Â Â Â Â local d = ESPObjects[p] if not d then return end
Â Â Â Â if d.Box then d.Box:Remove() end if d.Tracer then d.Tracer:Remove() end if d.Name then d.Name:Remove() end
Â Â Â Â if d.HBBG then d.HBBG:Remove() end if d.HB then d.HB:Remove() end
Â Â Â Â for _, l in ipairs(d.SkelLines or {}) do l:Remove() end if d.Highlight then d.Highlight:Destroy() end
Â Â Â Â ESPObjects[p] = nil
end
local function GetClosestPlayer()
Â Â Â Â local s = Env.Settings local visRad = Env.FOVSettings.Radius local intRad = 330 - visRad RequiredDistance = Env.FOVSettings.Enabled and intRad or 2000
Â Â Â Â local Closest, dist = nil, RequiredDistance
Â Â Â Â for _, p in Players:GetPlayers() do
Â Â Â Â Â Â Â Â if p == LP then continue end local c = p.Character local h = c and c:FindFirstChildOfClass("Humanoid")
Â Â Â Â Â Â Â Â if not (c and h and h.Health > 0 and c:FindFirstChild(s.LockPart)) then continue end
Â Â Â Â Â Â Â Â if s.TeamCheck and p.Team == LP.Team then continue end if table.find(Env.Blacklisted, p.Name) then continue end
Â Â Â Â Â Â Â Â local part = c[s.LockPart]
Â Â Â Â Â Â Â Â if s.WallCheck then
Â Â Â Â Â Â Â Â Â Â Â Â local parts = LP.Character and LP.Character:GetDescendants() or {}
Â Â Â Â Â Â Â Â Â Â Â Â for _, v in c:GetDescendants() do table.insert(parts, v) end
Â Â Â Â Â Â Â Â Â Â Â Â if #Camera:GetPartsObscuringTarget({part.Position}, parts) > 0 then continue end
Â Â Â Â Â Â Â Â end
Â Â Â Â Â Â Â Â local vec, onScr = Camera:WorldToViewportPoint(part.Position)
Â Â Â Â Â Â Â Â local d = (UIS:GetMouseLocation() - Vector2.new(vec.X, vec.Y)).Magnitude
Â Â Â Â Â Â Â Â if d < dist and onScr then dist = d Closest = p end
Â Â Â Â end
Â Â Â Â return Closest
end
