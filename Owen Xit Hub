-- Aimbot Module
getgenv().ExunysDeveloperAimbot = {
Â Â Â DeveloperSettings = {
Â Â Â Â Â Â UpdateMode = "RenderStepped",
Â Â Â Â Â Â RainbowSpeed = 1
Â Â Â },
Â Â Â Settings = {
Â Â Â Â Â Â Enabled = true,
Â Â Â Â Â Â TeamCheck = false,
Â Â Â Â Â Â AliveCheck = true,
Â Â Â Â Â Â WallCheck = false,
Â Â Â Â Â Â OffsetToMoveDirection = false,
Â Â Â Â Â Â OffsetIncrement = 15,
Â Â Â Â Â Â Sensitivity = 0,
Â Â Â Â Â Â Sensitivity2 = 3.5,
Â Â Â Â Â Â LockMode = 1,
Â Â Â Â Â Â LockPart = "Head",
Â Â Â Â Â Â TriggerKey = Enum.UserInputType.MouseButton2,
Â Â Â Â Â Â Toggle = false
Â Â Â },
Â Â Â FOVSettings = {
Â Â Â Â Â Â Enabled = true,
Â Â Â Â Â Â Visible = true,
Â Â Â Â Â Â Radius = 90,
Â Â Â Â Â Â NumSides = 60,
Â Â Â Â Â Â Thickness = 1,
Â Â Â Â Â Â Transparency = 1,
Â Â Â Â Â Â Filled = false,
Â Â Â Â Â Â RainbowColor = false,
