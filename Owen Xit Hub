})
VisualsTab:CreateSlider({
Name = "Line Thickness",
Range = {1,5},
Increment = 1,
CurrentValue = 2,
Flag = "ESP_Thickness",
Callback = function(v) ESPSettings.Thickness = v end
})
VisualsTab:CreateSlider({
Name = "Text Size",
Range = {12,24},
Increment = 1,
CurrentValue = 16,
Flag = "ESP_TextSize",
Callback = function(v) ESPSettings.TextSize = v end
})
-- Settings Tab
SettingsTab:CreateButton({
Name = "Guardar Configuraci贸n Ahora",
Callback = function()
Rayfield:SaveConfiguration()
Rayfield:Notify({Title = "Guardado", Content = "Configuraci贸n guardada manualmente", Duration = 4})
end
})
SettingsTab:CreateButton({
Name = "Cargar Configuraci贸n",
Callback = function()
Rayfield:LoadConfiguration()
Rayfield:Notify({Title = "Cargado", Content = "Configuraci贸n cargada", Duration = 4})
end
})
SettingsTab:CreateButton({
Name = "Cleanup ESP Only",
Callback = function()
for p in pairs(ESPObjects) do RemoveESP(p) end
Rayfield:Notify({Title = "ESP Limpio", Content = "Todos los ESP removidos", Duration = 3})
end
})
SettingsTab:CreateButton({
Name = "Reset Aimbot Only",
Callback = function()
if CurrentTween then CurrentTween:Cancel() CurrentTween = nil end
Env.Locked = nil Running = false
Rayfield:Notify({Title = "Aimbot Reset", Content = "Lock cancelado", Duration = 3})
end
})
-- Carga autom谩tica de configuraci贸n al iniciar (si existe)
Rayfield:LoadConfiguration()
print("Owen Xit Hub cargado - Key remota: " .. KeyUrl .. " = " .. CorrectKey)
Rayfield:Notify({Title = "隆Bienvenido!", Content = "Key cargada desde Pastebin remoto\nFOV 30 = super descarado\nConfiguraci贸n ahora se guarda y carga", Duration = 8})
