Â Â Â Â Â Â OutlineColor = Color3.fromRGB(0, 0, 0),
Â Â Â Â Â Â LockedColor = Color3.fromRGB(255, 150, 150)
Â Â Â },
Â Â Â Blacklisted = {},
Â Â Â FOVCircleOutline = Drawing.new("Circle"),
Â Â Â FOVCircle = Drawing.new("Circle")
}
local Environment = getgenv().ExunysDeveloperAimbot
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer
local Camera = workspace.CurrentCamera
Environment.FOVCircle.Visible = false
Environment.FOVCircleOutline.Visible = false
local RequiredDistance, Typing, Running, Animation = 2000, false, false, nil
local function GetRainbowColor()
Â Â Â return Color3.fromHSV(tick() % Environment.DeveloperSettings.RainbowSpeed / Environment.DeveloperSettings.RainbowSpeed, 1, 1)
end
local function ConvertVector(Vector)
Â Â Â return Vector2.new(Vector.X, Vector.Y)
end
local function CancelLock()
Â Â Â Environment.Locked = nil
Â Â Â if Animation then Animation:Cancel() end
Â Â Â Running = false
end
local function GetClosestPlayer()
Â Â Â local Settings = Environment.Settings
Â Â Â RequiredDistance = Environment.FOVSettings.Enabled and Environment.FOVSettings.Radius or 2000
Â Â Â local Closest = nil
Â Â Â for _, Value in ipairs(Players:GetPlayers()) do
Â Â Â Â Â Â local Character = Value.Character
Â Â Â Â Â Â local Humanoid = Character and Character:FindFirstChildOfClass("Humanoid")
Â Â Â Â Â Â if Value ~= LocalPlayer and not table.find(Environment.Blacklisted, Value.Name) and Character and Character:FindFirstChild(Settings.LockPart) and Humanoid then
Â Â Â Â Â Â Â Â Â local PartPosition = Character[Settings.LockPart].Position
Â Â Â Â Â Â Â Â Â if Settings.TeamCheck and Value.Team == LocalPlayer.Team then continue end
Â Â Â Â Â Â Â Â Â if Settings.AliveCheck and Humanoid.Health <= 0 then continue end
Â Â Â Â Â Â Â Â Â if Settings.WallCheck then
Â Â Â Â Â Â Â Â Â Â Â Â local Blacklist = LocalPlayer.Character:GetDescendants()
Â Â Â Â Â Â Â Â Â Â Â Â for _, v in ipairs(Character:GetDescendants()) do table.insert(Blacklist, v) end
Â Â Â Â Â Â Â Â Â Â Â Â if #Camera:GetPartsObscuringTarget({PartPosition}, Blacklist) > 0 then continue end
Â Â Â Â Â Â Â Â Â end
Â Â Â Â Â Â Â Â Â local Vector, OnScreen = Camera:WorldToViewportPoint(PartPosition)
Â Â Â Â Â Â Â Â Â Vector = ConvertVector(Vector)
Â Â Â Â Â Â Â Â Â local Distance = (UserInputService:GetMouseLocation() - Vector).Magnitude
Â Â Â Â Â Â Â Â Â if Distance < RequiredDistance and OnScreen then
Â Â Â Â Â Â Â Â Â Â Â Â RequiredDistance = Distance
Â Â Â Â Â Â Â Â Â Â Â Â Closest = Value
Â Â Â Â Â Â Â Â Â end
Â Â Â Â Â Â end
Â Â Â end
Â Â Â return Closest
end

-- ESP (tu versiÃ³n original sin cambios)
local ESPSettings = {
   Enabled = false,
   Boxes = true, Tracers = true, Names = true, Skeleton = true, HealthBars = true,
   TeamCheck = true,
   Rainbow = false,
   Color = Color3.fromRGB(0, 0, 0),
   Thickness = 2,
   TextSize = 16,
   MaxDistance = 300
}
local ESPObjects = {}
local R15Links = {{"Head","UpperTorso"}, {"UpperTorso","LowerTorso"}, {"LowerTorso","LeftUpperLeg"},{"LeftUpperLeg","LeftLowerLeg"},{"LeftLowerLeg","LeftFoot"}, {"LowerTorso","RightUpperLeg"},{"RightUpperLeg","RightLowerLeg"},{"RightLowerLeg","RightFoot"}, {"UpperTorso","LeftUpperArm"},{"LeftUpperArm","LeftLowerArm"},{"LeftLowerArm","LeftHand"}, {"UpperTorso","RightUpperArm"},{"RightUpperArm","RightLowerArm"},{"RightLowerArm","RightHand"}}
local R6Links = {{"Head","Torso"}, {"Torso","Left Arm"}, {"Torso","Right Arm"}, {"Torso","Left Leg"}, {"Torso","Right Leg"}}
local function CreateESP(p)
    if ESPObjects[p] then return end
    local char = p.Character
    if not char then return end
    local data = {Links = char:FindFirstChild("UpperTorso") and R15Links or R6Links}
    if ESPSettings.Boxes then data.Box = Drawing.new("Square") data.Box.Filled = false data.Box.Transparency = 1 data.Box.Thickness = ESPSettings.Thickness end
    if ESPSettings.Tracers then data.Tracer = Drawing.new("Line") data.Tracer.Thickness = 1.5 data.Tracer.Transparency = 1 end
    if ESPSettings.Names then data.Name = Drawing.new("Text") data.Name.Center = true data.Name.Outline = true data.Name.OutlineColor = Color3.new(0,0,0) data.Name.Font = 2 data.Name.Size = ESPSettings.TextSize data.Name.Transparency = 1 end
    if ESPSettings.HealthBars then
        data.HBBG = Drawing.new("Square") data.HBBG.Filled = true data.HBBG.Color = Color3.new(0,0,0) data.HBBG.Transparency = 0.6
        data.HB = Drawing.new("Square") data.HB.Filled = true data.HB.Transparency = 1
    end
    data.SkelLines = {}
    if ESPSettings.Skeleton then for _ = 1, #data.Links do local ln = Drawing.new("Line") ln.Thickness = 1.8 ln.Transparency = 0.7 table.insert(data.SkelLines, ln) end end
    data.Highlight = Instance.new("Highlight") data.Highlight.FillTransparency = 0.4 data.Highlight.OutlineTransparency = 0 data.Highlight.Parent = Camera
    ESPObjects[p] = data
end
local function RemoveESP(p)
    local d = ESPObjects[p] if not d then return end
    if d.Box then d.Box:Remove() end if d.Tracer then d.Tracer:Remove() end if d.Name then d.Name:Remove() end
    if d.HBBG then d.HBBG:Remove() end if d.HB then d.HB:Remove() end
    for _, l in ipairs(d.SkelLines or {}) do l:Remove() end if d.Highlight then d.Highlight:Destroy() end
    ESPObjects[p] = nil
end

local FrameCounter = 0
RunService.Stepped:Connect(function()
    FrameCounter += 1 if FrameCounter % 3 ~= 0 then return end
    local f = Environment.FOVSettings local s = Environment.Settings local fc = Environment.FOVCircle local fo = Environment.FOVCircleOutline
    local col = Environment.Locked and f.LockedColor or (f.RainbowColor and GetRainbowColor()) or f.Color
    if f.Enabled and s.Enabled then
        fc.Radius = f.Radius fc.NumSides = f.NumSides fc.Thickness = f.Thickness fc.Transparency = f.Transparency fc.Filled = f.Filled fc.Visible = f.Visible fc.Position = UserInputService:GetMouseLocation() fc.Color = col
        fo.Radius = f.Radius + f.Thickness fo.NumSides = f.NumSides fo.Thickness = f.Thickness + 1 fo.Transparency = f.Transparency fo.Visible = f.Visible fo.Position = UserInputService:GetMouseLocation() fo.Color = f.RainbowOutlineColor and GetRainbowColor() or f.OutlineColor
    else fc.Visible = false fo.Visible = false end
    if not ESPSettings.Enabled then return end
    for _, p in Players:GetPlayers() do
        if p == LocalPlayer then continue end local c = p.Character local root = c and c:FindFirstChild("HumanoidRootPart") local hum = c and c:FindFirstChild("Humanoid") local head = c and c:FindFirstChild("Head")
        if not (root and hum and head and hum.Health > 0) then RemoveESP(p) continue end
        local dist = (LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and (LocalPlayer.Character.HumanoidRootPart.Position - root.Position).Magnitude) or 9999
        if dist > ESPSettings.MaxDistance then RemoveESP(p) continue end
        if ESPSettings.TeamCheck and p.Team == LocalPlayer.Team then RemoveESP(p) continue end
        if not ESPObjects[p] then CreateESP(p) end local d = ESPObjects[p]
        local rPos, onScr = Camera:WorldToViewportPoint(root.Position) if not onScr then d.Box.Visible = false d.Tracer.Visible = false d.Name.Visible = false if d.HBBG then d.HBBG.Visible = false d.HB.Visible = false end for _,l in d.SkelLines do l.Visible = false end d.Highlight.Enabled = false continue end
        local color = ESPSettings.Rainbow and Color3.fromHSV((tick()*5 + p.UserId/255)%1,1,1) or ESPSettings.Color local vis = true
        if d.Box then d.Box.Visible = vis and ESPSettings.Boxes d.Box.Color = color local top = Camera:WorldToViewportPoint(head.Position + Vector3.new(0,0.5,0)) local bot = Camera:WorldToViewportPoint(root.Position - Vector3.new(0,4,0)) local h = math.abs(top.Y - bot.Y) local w = h*0.4 d.Box.Size = Vector2.new(w,h) d.Box.Position = Vector2.new(rPos.X - w/2, top.Y) end
        if d.Tracer then d.Tracer.Visible = vis and ESPSettings.Tracers d.Tracer.Color = color d.Tracer.From = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y*0.95) d.Tracer.To = Vector2.new(rPos.X, rPos.Y + 30) end
        if d.Name then d.Name.Visible = vis and ESPSettings.Names d.Name.Color = color d.Name.Text = p.Name.."\n"..math.floor(hum.Health).." HP | "..math.floor(dist).."u" local hpos = Camera:WorldToViewportPoint(head.Position) d.Name.Position = Vector2.new(rPos.X, hpos.Y - 40) end
        if d.HBBG and d.HB then
            d.HBBG.Visible = vis and ESPSettings.HealthBars d.HB.Visible = vis and ESPSettings.HealthBars
            local top = Camera:WorldToViewportPoint(head.Position) local bot = Camera:WorldToViewportPoint(root.Position - Vector3.new(0,4,0)) local barH = math.abs(top.Y - bot.Y) local boxW = d.Box and d.Box.Size.X or 50
            d.HBBG.Position = Vector2.new(rPos.X + boxW/2 + 6, top.Y) d.HBBG.Size = Vector2.new(4, barH)
            local perc = hum.Health / hum.MaxHealth d.HB.Color = Color3.fromRGB(255*(1-perc), 255*perc, 0) d.HB.Position = Vector2.new(d.HBBG.Position.X + 1.5, d.HBBG.Position.Y + barH*(1-perc)) d.HB.Size = Vector2.new(1, barH*perc)
        end
        if d.SkelLines then for i, line in ipairs(d.SkelLines) do local pair = d.Links[i] local p1 = c:FindFirstChild(pair[1]) local p2 = c:FindFirstChild(pair[2]) if p1 and p2 then local v1 = Camera:WorldToViewportPoint(p1.Position) local v2 = Camera:WorldToViewportPoint(p2.Position) line.From = Vector2.new(v1.X,v1.Y) line.To = Vector2.new(v2.X,v2.Y) line.Color = color line.Visible = vis and ESPSettings.Skeleton else line.Visible = false end end end
        d.Highlight.Adornee = c d.Highlight.FillColor = color d.Highlight.OutlineColor = color d.Highlight.Enabled = vis
    end
end)

RunService.Heartbeat:Connect(function()
    if not Running or not Environment.Settings.Enabled or not Environment.Locked then return end
    local char = Environment.Locked.Character local part = char and char:FindFirstChild(Environment.Settings.LockPart)
    if not part then CancelLock() return end
    local target = part.Position
    if Environment.Settings.LockMode == 2 then
        local scr = Camera:WorldToViewportPoint(target)
        mousemoverel((scr.X - UserInputService:GetMouseLocation().X)/Environment.Settings.Sensitivity2, (scr.Y - UserInputService:GetMouseLocation().Y)/Environment.Settings.Sensitivity2)
    else
        if Environment.Settings.Sensitivity > 0 then
            if Animation then Animation:Cancel() end
            Animation = TweenService:Create(Camera, TweenInfo.new(Environment.Settings.Sensitivity, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {CFrame = CFrame.new(Camera.CFrame.Position, target)})
            Animation:Play()
        else Camera.CFrame = CFrame.new(Camera.CFrame.Position, target) end
    end
end)

UserInputService.InputBegan:Connect(function(input)
    if Typing then return end
    local trig = Environment.Settings.TriggerKey
    if input.UserInputType == trig or input.KeyCode == trig then
        if Environment.Settings.Toggle then Running = not Running else Running = true end
        if Running then Environment.Locked = GetClosestPlayer() else CancelLock() end
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if Environment.Settings.Toggle or Typing then return end
    local trig = Environment.Settings.TriggerKey
    if input.UserInputType == trig or input.KeyCode == trig then Running = false CancelLock() end
end)

UserInputService.TextBoxFocused:Connect(function() Typing = true end)
UserInputService.TextBoxFocusReleased:Connect(function() Typing = false end)
Players.PlayerRemoving:Connect(RemoveESP)

-- INTERFACE
